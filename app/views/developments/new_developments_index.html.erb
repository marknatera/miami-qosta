<% title 'Property Development' %>

<section class="development-list" id="decription">
  <div class="container-fluid">
    <div class="row">
      <% @newdev.each do |n| %>
        <div class="col-xs-12 col-sm-4 hover-dev" style="padding-left:0;padding-right:0;">
          <div class="hover-development"></div>
          <h3><%= link_to "#{n.name}", n %></h3>
          <%= link_to image_tag(n.featured_image.url(:retina), class: 'img-responsive'), n %>
        </div>
      <% end %>
    </div>
  </div>
</section>
